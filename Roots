#include<stdio.h>
#include<math.h>
void main()
{
    double a,b,c,d,root1,root2;
    printf("ax^2+bx+c=0 Enter a b and c");
    scanf("%lf %lf %lf", &a, &b, &c);
    d= b * b - 4 * a * c;
    root1 = (-b + sqrt(d)) / (2 * a);
    root2 = (-b - sqrt(d)) / (2 * a);
    printf("root1 = %.lf and root2 = %.lf", root1, root2);
}
